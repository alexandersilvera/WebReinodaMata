{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/articles\\/edit\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/articles\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/tag\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		}
	]
}