		{
			"src": "^\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/nosotros\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/perfil\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/rss\\.xml\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/unsubscribe\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}