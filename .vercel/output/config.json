{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_server-islands\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/_image\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/articles\\/edit\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/articles\\/new\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/articles\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/metrics\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/newsletter\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/newsletter-test\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/settings\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/subscribers\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/sync\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/tag\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/contacto\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/instituto\\/?$",
			"dest": "_render"
