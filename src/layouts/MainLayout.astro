---
import BaseHead from "@/components/BaseHead.astro";
import Navigation from "@/components/Navigation.astro";

interface Props {
    title:string;
}
const { title } = Astro.props;
---


<!DOCTYPE html>
<html lang="es">
<BaseHead title={title} />
<body class="bg-dark-blue py-6">
    <main>
        <Navigation />
        <slot/>
    </main>
    
</body>
</html>